{
  "environment": "prod",
  "project_id": "my-project-prod",
  "region": "us-central1",
  "zone": "us-central1-a",
  
  "temp_location": "gs://my-project-prod-dataflow-temp/",
  "staging_location": "gs://my-project-prod-dataflow-staging/",
  
  "max_num_workers": 50,
  "disk_size_gb": 100,
  "machine_type": "n1-standard-4",
  "worker_machine_type": "n1-standard-4",
  
  "batch": {
    "user_events": {
      "input_bucket": "my-project-prod-data",
      "input_prefix": "user_events/raw/",
      "bigtable_instance": "prod-instance",
      "bigtable_table": "user_events",
      "bigtable_column_family": "cf",
      "batch_size": 1000,
      "bigquery_dataset": "analytics_prod",
      "enable_autoscaling": true,
      "autoscaling_algorithm": "THROUGHPUT_BASED"
    },
    "transaction_data": {
      "input_bucket": "my-project-prod-data",
      "input_prefix": "transactions/raw/",
      "bigtable_instance": "prod-instance",
      "bigtable_table": "transactions",
      "bigtable_column_family": "cf",
      "batch_size": 1000,
      "bigquery_dataset": "finance_prod"
    },
    "audit_logs": {
      "input_bucket": "my-project-prod-data",
      "input_prefix": "audit_logs/raw/",
      "bigtable_instance": "prod-instance",
      "bigtable_table": "audit_logs",
      "bigtable_column_family": "cf",
      "batch_size": 1000,
      "bigquery_dataset": "security_prod"
    }
  },
  
  "streaming": {
    "real_time_events": {
      "subscription": "projects/my-project-prod/subscriptions/real-time-events-sub",
      "topic": "projects/my-project-prod/topics/real-time-events",
      "dead_letter_topic": "projects/my-project-prod/topics/real-time-events-dlq",
      "alert_topic": "projects/my-project-prod/topics/alerts",
      "bigtable_instance": "prod-instance",
      "bigtable_table": "real_time_events",
      "bigtable_column_family": "cf",
      "bigtable_batch_size": 500,
      "bigtable_flush_interval_seconds": 5,
      "window_size_seconds": 300,
      "allowed_lateness_seconds": 3600,
      "enable_streaming_engine": true,
      "autoscaling_algorithm": "THROUGHPUT_BASED"
    },
    "clickstream_analytics": {
      "subscription": "projects/my-project-prod/subscriptions/clickstream-sub",
      "topic": "projects/my-project-prod/topics/clickstream", 
      "dead_letter_topic": "projects/my-project-prod/topics/clickstream-dlq",
      "bigtable_instance": "prod-instance",
      "bigtable_table": "clickstream",
      "bigtable_column_family": "cf",
      "window_size_seconds": 300,
      "allowed_lateness_seconds": 3600
    },
    "iot_sensors": {
      "subscription": "projects/my-project-prod/subscriptions/iot-sensors-sub",
      "topic": "projects/my-project-prod/topics/iot-sensors",
      "dead_letter_topic": "projects/my-project-prod/topics/iot-sensors-dlq",
      "bigtable_instance": "prod-instance",
      "bigtable_table": "sensor_data",
      "bigtable_column_family": "cf",
      "window_size_seconds": 60,
      "allowed_lateness_seconds": 300
    },
    "fraud_detection": {
      "subscription": "projects/my-project-prod/subscriptions/fraud-detection-sub",
      "topic": "projects/my-project-prod/topics/transactions",
      "dead_letter_topic": "projects/my-project-prod/topics/fraud-detection-dlq",
      "alert_topic": "projects/my-project-prod/topics/fraud-alerts",
      "bigtable_instance": "prod-instance",
      "bigtable_table": "fraud_scores",
      "bigtable_column_family": "cf",
      "window_size_seconds": 60,
      "ml_model_endpoint": "projects/my-project-prod/locations/us-central1/endpoints/fraud-model-prod"
    }
  },
  
  "monitoring": {
    "enable_monitoring": true,
    "monitoring_namespace": "dataflow_prod",
    "alert_email": "sre-team@example.com",
    "slack_webhook": "https://hooks.slack.com/services/prod-alerts",
    "pagerduty_key": "your-pagerduty-integration-key",
    "enable_stackdriver_profiling": true
  },
  
  "security": {
    "service_account": "dataflow-prod@my-project-prod.iam.gserviceaccount.com",
    "use_public_ips": false,
    "network": "projects/my-project-prod/global/networks/dataflow-vpc",
    "subnetwork": "projects/my-project-prod/regions/us-central1/subnetworks/dataflow-subnet",
    "kms_key": "projects/my-project-prod/locations/us-central1/keyRings/dataflow-ring/cryptoKeys/dataflow-key"
  },
  
  "dataflow_options": {
    "use_runner_v2": true,
    "sdk_container_image": "gcr.io/my-project-prod/dataflow-python:latest",
    "sdk_location": "container",
    "enable_streaming_engine": true,
    "flexrs_goal": "SPEED_OPTIMIZED"
  },
  
  "performance": {
    "worker_disk_type": "compute.googleapis.com/projects/my-project-prod/zones/us-central1-a/diskTypes/pd-ssd",
    "use_preemptible_workers": false,
    "enable_shuffle_service": true,
    "streaming_engine_version": "v1"
  }
}